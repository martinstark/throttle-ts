{
  "name": "@martinstark/throttle-ts",
  "version": "1.2.2",
  "description": "Correctly typed generic throttle function",
  "typings": "dist/typings/index.d.ts",
  "main": "dist/throttle-ts.min.js",
  "private": false,
  "license": "MIT",
  "files": [
    "/dist",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:martinstark/throttle-ts.git"
  },
  "scripts": {
    "build": "yarn build:clean && webpack --config webpack.common.js && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:clean": "rimraf dist/*",
    "fix": "eslint --fix .",
    "lintstaged": "lint-staged",
    "pretty": "prettier --write .",
    "ts": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-typescript": "7.13.0",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "babel-loader": "8.2.2",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint_d": "10.0.4",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "typescript": "4.2.3",
    "webpack": "5.24.4",
    "webpack-cli": "4.5.0",
    "webpack-merge": "5.7.3"
  },
  "keywords": [
    "throttle",
    "typescript"
  ]
}
